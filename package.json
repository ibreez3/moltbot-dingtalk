{
  "name": "moltbot-dingtalk",
  "version": "1.0.0",
  "type": "module",
  "description": "OpenClaw DingTalk channel plugin - AI Card streaming with session management",
  "license": "ISC",
  "files": [
    "index.ts",
    "src",
    "openclaw.plugin.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibreez3/moltbot-dingtalk.git"
  },
  "keywords": [
    "openclaw",
    "moltbot",
    "dingtalk",
    "钉钉",
    "chatbot",
    "ai",
    "claude"
  ],
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "dingtalk",
      "label": "DingTalk",
      "selectionLabel": "DingTalk (钉钉)",
      "docsPath": "/channels/dingtalk",
      "docsLabel": "dingtalk",
      "blurb": "DingTalk enterprise messaging with AI Card streaming support.",
      "aliases": [
        "钉钉"
      ],
      "order": 60
    },
    "install": {
      "npmSpec": "moltbot-dingtalk",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "dependencies": {
    "axios": "^1.13.4",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/ws": "^8.18.1",
    "openclaw": "2026.1.29",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.29"
  }
}
